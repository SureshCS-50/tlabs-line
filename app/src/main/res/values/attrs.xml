<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--for application typeface-->
    <!-- Define the values for the attribute -->
    <attr name="typeface" format="enum">
        <enum name="roboto_regular" value="0" />
        <enum name="robotocondensed_bold" value="1" />
        <enum name="roboto_light" value="2" />
        <enum name="roboto_medium" value="3" />
    </attr>
    <!-- Tell Android that the class "TypefaceTextView" can be styled,
         and which attributes it supports -->
    <declare-styleable name="TypefaceTextView">
        <attr name="typeface" />
    </declare-styleable>
</resources>